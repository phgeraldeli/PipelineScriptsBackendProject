{  
    "family": "@REPLACE_FAMILY@",
    "networkMode": "@REPLACE_NETWORK_MODE@",
    "memory": "1024",
    "cpu": "512",
    "executionRoleArn": "arn:aws:iam::731735707548:role/ecsTaskExecutionRole",
    "requiresCompatibilities": [		
        "FARGATE",
        "EC2"	
    ],
    "containerDefinitions": [
      {            
        "logConfiguration": {
          "logDriver": "awslogs",        
          "options": {
            "awslogs-group": "/ecs/@REPLACE_FAMILY@",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        },      
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],                             
        "mountPoints": [
          {          
            "containerPath": "/tmp/teste",
            "sourceVolume": "testefargatevolume"
          }
        ],                              
        "volumesFrom": [],      
        "image": "@REPLACE_IMG@",                                    
        "essential": true,                                               
        "name": "@REPLACE_CONTAINER@"
      }
    ],
   
    "volumes": [
      {      
        "efsVolumeConfiguration": {        
          "fileSystemId": "fs-9aa85f6c",
          "transitEncryption": "DISABLED",
          "rootDirectory": "/"
        },
        "name": "testefargatevolume"
      }
    ]
  }